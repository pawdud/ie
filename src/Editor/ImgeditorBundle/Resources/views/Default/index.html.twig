{% extends "EditorImgeditorBundle::layout.html.twig" %}

{% block body %}
<div class="panel panel-default">    
    <div class="panel-heading">
        1 Wybierz zdjęcie
    </div>    
    <div class="panel-body">
        <form class="form" method="post" action="{{path('pr_create')}}" enctype="multipart/form-data">
            <div class="form-group">
                <input type="file" name="file" value="Wybierz zdjęcie" />
            </div>

            <div class="form-group">
                <button type="submit" class="btn btn-success">Utwórz projekt</button>
            </div>
        </form>
    </div>    
</div>
{% endblock %}